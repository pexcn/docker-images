{
    "servers": [
        {
            "server": "0.0.0.0",
            "server_port": 1776,
            "password": "${SHADOWSOCKS_PASSWORD_1}",
            "method": "2022-blake3-aes-128-gcm"
        },
        {
            "server": "0.0.0.0",
            "server_port": 1984,
            "password": "${SHADOWSOCKS_PASSWORD_2}",
            "method": "2022-blake3-chacha8-poly1305"
        },
        {
            "server": "0.0.0.0",
            "server_port": 443,
            "password": "${SHADOWSOCKS_PASSWORD_3}",
            "method": "chacha20-ietf-poly1305",
            "plugin": "xray-plugin",
            "plugin_opts": "server;tls;fast-open;mode=grpc;host=example.com"
        }
    ],

    "mode": "tcp_and_udp",
    "timeout": 3600,
    "udp_timeout": 300,
    "keep_alive": 300,
    "nofile": 1048576,
    "no_delay": true,
    "fast_open": true
}
